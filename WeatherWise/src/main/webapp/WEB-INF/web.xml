<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>WeatherWise</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
    <welcome-file>login.jsp</welcome-file>
    <welcome-file>Contact.jsp</welcome-file>
    <welcome-file>aboutus.jsp</welcome-file>
    <welcome-file>profile.jsp</welcome-file>
    <welcome-file>Register.jsp</welcome-file>
    <welcome-file>admindashboard.jsp</welcome-file>
    <welcome-file>userdashboard.jsp</welcome-file>
  </welcome-file-list>
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>/css/*</url-pattern>
    <url-pattern>/resources/*</url-pattern>
  </servlet-mapping>
  
</web-app>